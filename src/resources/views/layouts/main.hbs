<!DOCTYPE html>
<html lang="en" class="light-style layout-menu-fixed" dir="ltr" data-theme="theme-default"
    data-template="vertical-menu-template-free">

    {{> header}}

<body class="bg--outside">

        {{#if notInMain}}
        {{{body}}}
        {{else}}
        <div class="layout-wrapper layout-content-navbar">
            <div class="layout-container">
                <div class="layout-page">
                    <div class="content-wrapper">
                        {{> bar/sidebar}}
                        {{> bar/navbar}}

                        {{{body}}}

                        {{> footer}}
                        <div class="content-backdrop fade"></div>
                    </div>

                </div>
            </div>

            <div class="overlay position-fixed top-0 bottom-0 end-0 start-0 d-none" id="overlay"
                style="background-color: rgba(0, 0, 0, 0.1);">
                <div class="spinner position-relative top-50 end-50 bottom-50 start-50">
                    <div class="spinner-border spinner-border-lg text-primary" role="status"></div>
                    <p class="visually-hidde" style="font-size: 18px;">Loading...</p>
                </div>
            </div>
            {{> overlay/overlay}}
        </div>
        <script>
            jsInHome()
        </script>

        {{/if}}

        {{!-- {{> footer}} --}}

        {{!--
    </div> --}}
    {{> corejs}}
</body>

</html>